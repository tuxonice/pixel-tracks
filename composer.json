{
    "name": "tuxonice/pixel-track",
    "keywords": [
        "PHP",
        "gpx",
        "framework"
    ],
    "authors": [
        {
            "name": "Helder Correia",
            "email": "tuxboy@tlab.pt"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PixelTrack\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "sibyx/phpgpx": "1.3.0",
        "ext-json": "*",
        "vlucas/phpdotenv": "5.5.0",
        "symfony/http-foundation": "6.3.2",
        "twig/twig": "3.7.0",
        "symfony/console": "6.3.2",
        "monolog/monolog": "3.4.0",
        "ext-sqlite3": "*",
        "nikic/fast-route": "1.3.0",
        "ext-dom": "*",
        "ext-libxml": "*",
        "php-di/php-di": "7.0.3",
        "symfony/cache": "6.3.2",
        "phpmailer/phpmailer": "6.8.0",
        "illuminate/support": "10.17.1",
        "symfony/uid": "6.3.0"
    },
    "require-dev": {
        "symfony/var-dumper": "6.3.3",
        "squizlabs/php_codesniffer": "3.7.2",
        "phpstan/phpstan": "1.10.27",
        "codeception/codeception": "5.0.10",
        "codeception/module-phpbrowser": "3.0.0",
        "codeception/module-asserts": "3.0.0",
        "dg/bypass-finals": "1.4.1"
    },
    "scripts": {
        "static-analysis": [
            "phpcs",
            "phpstan"
        ],
        "tests": [
            "@static-analysis",
            "codecept run"
        ]
    }
}
