{
    "name": "tuxonice/pixel-track",
    "keywords": [
        "PHP",
        "gpx",
        "framework"
    ],
    "authors": [
        {
            "name": "Helder Correia",
            "email": "tuxboy@tlab.pt"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PixelTrack\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "sibyx/phpgpx": "1.3.0",
        "ext-json": "*",
        "vlucas/phpdotenv": "5.6.1",
        "symfony/http-foundation": "6.4.15",
        "twig/twig": "3.15.0",
        "symfony/console": "6.4.15",
        "monolog/monolog": "3.8.0",
        "ext-sqlite3": "*",
        "nikic/fast-route": "1.3.0",
        "ext-dom": "*",
        "ext-libxml": "*",
        "php-di/php-di": "7.0.7",
        "symfony/cache": "6.4.14",
        "illuminate/support": "11.32.0",
        "symfony/uid": "6.4.13",
        "symfony/http-client": "6.4.15",
        "tuxonice/transfer-objects": "v1.1.2",
        "phpmailer/phpmailer": "6.9.2",
        "doctrine/dbal": "4.2.1"
    },
    "require-dev": {
        "symfony/var-dumper": "6.4.15",
        "squizlabs/php_codesniffer": "3.11.1",
        "phpstan/phpstan": "2.0.2",
        "codeception/codeception": "5.1.2",
        "codeception/module-phpbrowser": "3.0.1",
        "codeception/module-asserts": "3.0.0",
        "dg/bypass-finals": "1.8.0",
        "codeception/module-db": "3.1.4"
    },
    "scripts": {
        "static-analysis": [
            "phpcs",
            "phpstan"
        ],
        "tests": [
            "@static-analysis",
            "codecept run"
        ]
    }
}
