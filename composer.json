{
    "name": "tuxonice/pixel-track",
    "keywords": [
        "PHP",
        "gpx",
        "framework"
    ],
    "authors": [
        {
            "name": "Helder Correia",
            "email": "tuxboy@tlab.pt"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PixelTrack\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "sibyx/phpgpx": "1.3.0",
        "ext-json": "*",
        "vlucas/phpdotenv": "5.6.0",
        "symfony/http-foundation": "6.4.4",
        "twig/twig": "3.9.2",
        "symfony/console": "6.4.6",
        "monolog/monolog": "3.6.0",
        "ext-sqlite3": "*",
        "nikic/fast-route": "1.3.0",
        "ext-dom": "*",
        "ext-libxml": "*",
        "php-di/php-di": "7.0.6",
        "symfony/cache": "6.4.6",
        "illuminate/support": "11.4.0",
        "symfony/uid": "6.4.3",
        "symfony/http-client": "6.4.6",
        "tuxonice/transfer-objects": "v1.1.2",
        "phpmailer/phpmailer": "6.9.1"
    },
    "require-dev": {
        "symfony/var-dumper": "6.4.6",
        "squizlabs/php_codesniffer": "3.9.1",
        "phpstan/phpstan": "1.10.67",
        "codeception/codeception": "5.1.2",
        "codeception/module-phpbrowser": "3.0.1",
        "codeception/module-asserts": "3.0.0",
        "dg/bypass-finals": "1.6.0"
    },
    "scripts": {
        "static-analysis": [
            "phpcs",
            "phpstan"
        ],
        "tests": [
            "@static-analysis",
            "codecept run"
        ]
    }
}
