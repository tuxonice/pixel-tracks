{
    "name": "tuxonice/pixel-track",
    "version": "v0.4.0",
    "keywords": [
        "PHP",
        "gpx",
        "framework"
    ],
    "authors": [
        {
            "name": "Helder Correia",
            "email": "tuxboy@tlab.pt"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PixelTrack\\": "src/"
        }
    },
    "require": {
        "php": "8.4.3",
        "sibyx/phpgpx": "1.3.0",
        "ext-json": "*",
        "vlucas/phpdotenv": "5.6.1",
        "symfony/http-foundation": "6.4.16",
        "twig/twig": "3.18.0",
        "symfony/console": "6.4.17",
        "monolog/monolog": "3.8.1",
        "ext-sqlite3": "*",
        "nikic/fast-route": "1.3.0",
        "ext-dom": "*",
        "ext-libxml": "*",
        "php-di/php-di": "7.0.7",
        "symfony/cache": "6.4.16",
        "illuminate/support": "11.38.2",
        "symfony/uid": "6.4.13",
        "symfony/http-client": "6.4.17",
        "tuxonice/transfer-objects": "v1.1.2",
        "phpmailer/phpmailer": "6.9.3",
        "doctrine/dbal": "4.2.2"
    },
    "require-dev": {
        "symfony/var-dumper": "6.4.15",
        "squizlabs/php_codesniffer": "3.11.2",
        "phpstan/phpstan": "2.1.1",
        "codeception/codeception": "5.1.2",
        "codeception/module-phpbrowser": "3.0.1",
        "codeception/module-asserts": "3.0.0",
        "dg/bypass-finals": "1.9.0",
        "codeception/module-db": "3.1.4"
    },
    "scripts": {
        "post-install-cmd": [
            "chmod -R 755 vendor",
            "find vendor -type f -exec chmod 644 {} \\;",
            "find vendor -type d -exec chmod 755 {} \\;",
            "find vendor/bin -type f -exec chmod 755 {} \\;"
        ],
        "static-analysis": [
            "phpcs",
            "phpstan"
        ],
        "tests": [
            "@static-analysis",
            "codecept run"
        ]
    }
}
